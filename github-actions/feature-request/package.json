{
  "name": "feature-request",
  "version": "0.0.0",
  "private": true,
  "description": "Triages GitHub issues",
  "main": "lib/bundle.js",
  "scripts": {
    "build": "esbuild src/main.ts --bundle --outfile=lib/bundle.js --platform=node --target=node12",
    "test": "ts-node -P tsconfig.json node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/dev-infra.git"
  },
  "author": "Angular",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^2.1.1",
    "github-app-installation-token": "^1.1.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.3",
    "esbuild": "^0.11.23",
    "jasmine": "^3.6.4",
    "nyc": "^15.1.0",
    "tslib": "1.10.0",
    "ts-node": "^9.1.1",
    "typescript": "4.1.3"
  }
}
